CREATE TABLE books 
    (
        book_id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
        title VARCHAR(100),
        author_fname VARCHAR(100),
        author_lname VARCHAR(100),
        category VARCHAR(100),
        release_year INTEGER,
        pages INTEGER,
        price DECIMAL,
        picture BLOB
    );

